pico-8 cartridge // http://www.pico-8.com
version 22
__lua__
function _init()
 x=0 t=0 speed=0.05
 frame=0
 aspeed=0.1
 atime=0
end

function _update()
 t+=speed
 x+=(sin(t)+1)
 if x>127 then
  x-=127
 end
 atime+=speed
 while atime>=1 do
  atime-=1
 end
 
 frame=flr(atime*2)*2
end

function _draw()
 cls()
 spr(17+frame,x,48,2,2)
end
